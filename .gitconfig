[color]
  ui = auto
[merge]
  tool = vimdiff
[alias]
  unstage = reset HEAD --
  ll = log --graph --pretty=format:'%Cred%h - %Creset%s%Creset  %Cgreen(%cr)%Creset %C(bold blue) <%an>' --abbrev-commit --date=relative
  cleanup = remote prune origin
[diff]
  tool = vimdiff
[difftool]
  prompt = false
[user]
  name = Aleksey Lashin
  email = aleksey.lashin@gmail.com
[core]
  autocrlf = input
  quotepath = false
[pull]
  rebase = true
[credential]
  helper = osxkeychain
